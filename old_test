import re
from functools import reduce
from operator import add
patt = re.compile("/akt-prawny/DZU")
patt1 = re.compile("\.html")
alist = 'href="/akt-prawny/DZU.2018.180.0001784,rozporzadzenie-ministra-rolnictwa-i-rozwoju-wsi-w-sprawie-szczegolowych-warunkow-i-trybu-przyznawania-pomocy-finansowej-w-ramach-dzialania-rolnictwo-ekologiczne-objetego-programem-rozwoju-obszarow-wie.html"'

print(patt.search(alist).span()[0])
print(alist[6:len(alist) - 1])
print(patt1.search(alist).span()[1])
a = [["a"], ["b"], ["c"]]
print(reduce(add, a))
